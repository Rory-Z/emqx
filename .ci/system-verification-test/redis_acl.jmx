        <hashTree>
          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="get ssl content" enabled="true">
            <stringProp name="TestPlan.comments">最终得到参数名：ca, redis_key, redis_cert</stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import java.io.File;
import java.io.FileInputStream;
import java.io.InputStreamReader;
import java.io.BufferedReader;

File f_ca = new File(&quot;${ca_route}/ca.pem&quot;);
File f_key = new File(&quot;${ca_route}/redis-key.pem&quot;);
File f_cert = new File(&quot;${ca_route}/redis-cert.pem&quot;);
BufferedReader br_ca = new BufferedReader(new InputStreamReader(new FileInputStream(f_ca),&quot;UTF-8&quot;));
BufferedReader br_key = new BufferedReader(new InputStreamReader(new FileInputStream(f_key),&quot;UTF-8&quot;));
